

public class Weapon extends Item {
  
  
  private int damage;
  
  public int getDamage(){
    
    return damage;
  }
  
  public void set damage(int d){
    
    damage = d;
  }
  
  public Item(int damage, String name, int weight){
    
    super(name);
    super(weight);
    this.damage = damage;
    
  }
}
    
  