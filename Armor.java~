public class Armor extends Item{
  
  private int defense;
  
  public int getDefense(){
    
    return defense;
  }
  
  public void setDefense(int def){
    
    defense = def;
  }
  
    public Armor(int defense, String name, int weight){
    
    super(name + weight);
    this.defense = defense;
    
  }
     public void Examine()
  {
    System.out.println("Name:" + super.getName() + " Weight: " + super.getWeight() + " Defense: " + defense);
  }
}